@{
    int min = (int)ViewData["min"];
    int max = (int)ViewData["max"];

    ViewData["Title"] = "Numbers";
}
<h1>Numbers</h1>
<p>Обед до 12:20</p>
<ul class="list-unstyled">
    @for(int i = min; i<=max; i++)
    {
        <li>@i</li>
    }
</ul>
